---
layout: null
permalink: /sitemap.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url><loc>{{ site.url }}{{ site.baseurl }}/</loc></url>
  <url><loc>{{ site.url }}{{ '/episodes/' | relative_url }}</loc></url>
  {% for p in site.pages %}
  <url><loc>{{ site.url }}{{ p.url }}</loc></url>
  {% endfor %}
  {% for e in site.episodes %}
  <url><loc>{{ site.url }}{{ e.url }}</loc></url>
  {% endfor %}
</urlset>
